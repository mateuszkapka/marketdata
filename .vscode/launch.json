    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'marketdata'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=marketdata",
                    "--package=marketdata"
                ],
                "filter": {
                    "name": "marketdata",
                    "kind": "bin"
                }
            },
            "args": ["NASDAQ"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'symbology'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=symbology",
                ],
                "filter": {
                    "name": "symbology",
                    "kind": "bin"
                }
            },
            "args": ["WSE"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'marketdata'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=marketdata",
                    "--package=marketdata"
                ],
                "filter": {
                    "name": "marketdata",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        } ,
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug 'aggregates'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=aggregates",
                ],
                "filter": {
                    "name": "aggregates",
                    "kind": "bin"
                }
            },
            "args": ["wse"],
            "cwd": "${workspaceFolder}"
        }
    ]
}